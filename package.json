{
    "name": "eft-stream-deck-timer-plugin",
    "version": "1.0.0",
    "description": "Escape from Tarkov raid timer plugin for Stream Deck",
    "main": "src/plugin.ts",
    "type": "module",
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w --watch.onEnd=\"streamdeck restart com.gtech9971.eft-stream-deck-timer-plugin\"",
        "start": "npm run watch"
    },
    "keywords": [
        "streamdeck",
        "plugin", 
        "escape-from-tarkov",
        "timer",
        "gaming"
    ],
    "author": "gtech9971",
    "license": "MIT",
    "devDependencies": {
        "@elgato/cli": "^1.4.0",
        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-node-resolve": "^15.2.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.0",
        "@tsconfig/node20": "^20.1.2",
        "@types/node": "~20.15.0",
        "rollup": "^4.0.2",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@elgato/streamdeck": "^1.0.0"
    }
}